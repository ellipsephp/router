{
    "name": "ellipse/router",
    "type": "library",
    "description": "Router allowing to build Psr-15 dispatcher based on a Psr-7 request",
    "keywords": [
        "router"
    ],
    "license": "MIT",
    "homepage": "https://github.com/ellipsephp/router",
    "support": {
        "issues": "https://github.com/ellipsephp/router/issues",
        "source": "https://github.com/ellipsephp/router"
    },
    "authors": [
        {
            "name": "pmall",
            "email": "pierre.mallinjoud@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.1",
        "psr/http-message": "^1.0",
        "http-interop/http-server-handler": "^1.0",
        "ellipse/router-adapter": "^1.0"
    },
    "require-dev": {
        "kahlan/kahlan": "^4.0",
        "eloquent/phony-kahlan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Ellipse\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
